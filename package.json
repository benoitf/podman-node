  "devDependencies": {
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.18.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "typescript": "^4.0"
  }
