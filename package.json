{
  "name": "podman-node",
  "version": "0.0.1",
  "description": "OpenAPI client for podman-node",
  "author": "benoitf",
  "repository": {
    "type": "git",
    "url": "https://github.com/benoitf/podman-node.git"
  },
  "keywords": [
    "typescript",
    "podman",
    "openapi-client",
    "openapi-generator"
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
